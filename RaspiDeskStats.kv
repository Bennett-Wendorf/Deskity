<Screen_Manager>:
    MainScreen:
        id: main_screen
    SettingsScreen:
        id: settings_screen

<MainScreen>:
    name: 'main_screen'
    BoxLayout:
        orientation: "horizontal"
        BoxLayout:
            orientation: "vertical"
            BoxLayout:
                orientation: "horizontal"
                CheckBox:

            Button:
                text: "App Location Placeholder"
        ToDoWidget:
            id: to_do_widget
            name: 'to_do_widget'
            Button:
                text: "Test"

<SettingsScreen>:
    name: 'settings_screen'
    BoxLayout:
        Button:
            text: 'GoTo Main'
            on_press: root.manager.switchToMain()
